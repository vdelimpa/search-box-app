{
  "name": "search-box-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-testing": "^3.1.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "chromedriver": "^79.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "jest-enzyme": "^7.1.2",
    "nightwatch": "^1.3.2",
    "nightwatch-vrt": "^0.2.10",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "react-spinners": "^0.8.0",
    "react-test-renderer": "^16.12.0"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && npm run build-css -- -w -r",
    "build-app": "react-scripts build",
    "start-app": "react-scripts start",
    "build": "npm-run-all -p build-css build-app",
    "start": "npm-run-all -p watch-css start-app",
    "lint": "eslint src/**/*.js",
    "test": "react-scripts test --verbose --coverage --watchAll=false",
    "test-watch": "react-scripts test --verbose",
    "test-e2e": "nightwatch -c nightwatch.json",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0"
  }
}
